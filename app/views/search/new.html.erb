<%= render partial: 'shared/navbar' %>

<h2>Search for an artist:</h2>

<div class="container">
  <%= form_for :artsy, url: "/search", method: "get" do |f| %>
    <%= f.label :artist %>
    <%= f.text_field :artist %>
    <%= f.submit "Search Artsy", class: "btn indigo darken-4" %>
  <% end %>
</div>
