<%= render partial: 'shared/navbar' %>


<div class="container search">
  <h5>Search for an artist:</h5>
  <div class="search-form">
    <%= form_for :artsy, url: "/search", method: "get" do |f| %>
    <%= f.text_field :artist %>
    <%= f.label :artist %>
    <br><br>
    <%= f.submit "Search Artsy", class: "btn indigo darken-4" %>
    <% end %>
  </div>
</div>
